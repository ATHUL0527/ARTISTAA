    function handleDataHrefClick(event){
      event.preventDefault();
      const scrollToId = (event.currentTarget.getAttribute("data-href")).replace("#", "");
      const scrollToDiv = document.getElementById(scrollToId);
      if(scrollToDiv){
        const headerHeight = document.getElementById("shopify-section-header");
        const distance = scrollToDiv.offsetTop - (headerHeight.offsetHeight + 30)
        console.log("Same page scroll");
        window.scrollTo({
          top: distance,
          behaviour: "smooth"
        })
      }
      else{
        console.log("redirect then scroll");
        const fallbackUrl = event.currentTarget.getAttribute("data-fallback");
        window.localStorage.setItem("is_redirected", scrollToId);
        window.location.href = fallbackUrl;
      }
    }

    
    const dataHrefs = document.querySelectorAll("[data-href]");
    if(dataHrefs.length){
      dataHrefs.forEach((dataHref)=> dataHref.addEventListener("click", handleDataHrefClick.bind(this)));
    }

    const was_redirected = window.localStorage.getItem("is_redirected") ? window.localStorage.getItem("is_redirected") : false;
    if(was_redirected){
      const scrollToDiv = document.getElementById(was_redirected);
      if(scrollToDiv){
        console.log("Was redirected");
         window.localStorage.removeItem("is_redirected")
        const headerHeight = document.getElementById("shopify-section-header");
